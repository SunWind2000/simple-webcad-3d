{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "standard-with-typescript",
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "semi": "warn",
    "quotes": "warn",
    "eol-last": "error",
    "indent": [
      "error",
      2
    ]
  }
}